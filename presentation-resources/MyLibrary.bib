  langid = {english},
  keywords = {Climate-change mitigation,Socioeconomic scenarios},
  file = {/home/max/Zotero/storage/HBEM3NJX/Creutzig et al. - 2022 - Demand-side solutions to climate change mitigation.pdf;/home/max/Zotero/storage/RQHD3QQY/s41558-021-01219-y.html}
