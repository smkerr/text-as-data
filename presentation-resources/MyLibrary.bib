@incollection{yamagata_urban_2016,
  title = {Urban {{Economics Model}} for {{Land-Use Planning}}},
  booktitle = {Urban {{Resilience}}},
  author = {Yamagata, Yoshiki and Seya, Hajime and Murakami, Daisuke},
