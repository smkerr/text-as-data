@article{lamb_what_2020,
  title = {What Are the Social Outcomes of Climate Policies? {{A}} Systematic Map and Review of the Ex-Post Literature},
  shorttitle = {What Are the Social Outcomes of Climate Policies?},
  author = {Lamb, William F. and Antal, Mikl{\'o}s and Bohnenberger, Katharina and {Brand-Correa}, Lina I. and {M{\"u}ller-Hansen}, Finn and Jakob, Michael and Minx, Jan C. and Raiser, Kilian and Williams, Laurence and Sovacool, Benjamin K.},
