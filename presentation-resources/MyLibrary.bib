@article{sheller_city_2000,
  title = {The {{City}} and the {{Car}}},
  author = {Sheller, Mimi and Urry, John},
  year = {2000},
  month = dec,
  journal = {International Journal of Urban and Regional Research},
  volume = {24},
  number = {4},
  pages = {737--757},
